/**index.wxss**/

.container {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #fff;
  color: #939393;
}

.control {
  position: fixed;
  right: 32rpx;
  bottom: 32rpx;
  display: flex;
  z-index: 999;
  flex-direction: row;
  background: white;
  height: 96rpx;
  width: 256rpx;
    box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 3px 10px 0 rgba(0, 0, 0, 0.19);
  border-bottom: 1rpx solid #dbdbdb;
  border-radius: 25rpx;
}

.control .text {
  height: 96rpx;
  width: 128rpx;
  color: black;
  font-size: 1.6rem;
  text-align: center;
}
